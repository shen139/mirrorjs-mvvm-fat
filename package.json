{
  "name": "mirrorjs-mvvm-fat",
  "version": "0.0.1",
  "description": "mirrorjs sample fat client (MVVM)",
  "main": "./src",
  "directories": {
    "bin": "./build",
    "lib": "./src"
  },
  "scripts": {
