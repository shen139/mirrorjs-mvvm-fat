    "grunt": "^1.0.1",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-less-browserify": "0.0.3",
    "parallelshell": "^2.0.0",
    "mirrorjs": "^0.3.0",
    "mirrorjs-widgets": "0.0.1"
  },
  "engines": {
    "node": ">= 0.10",
    "npm": ">= 2.1"
